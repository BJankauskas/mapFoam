defaultFieldValues
(
    volScalarFieldValue moment.0.populationBalance 1.0
    volScalarFieldValue moment.1.populationBalance 2.708217669
    volScalarFieldValue moment.2.populationBalance 8.951330468
    volScalarFieldValue moment.3.populationBalance 35.95258119
    volScalarFieldValue moment.4.populationBalance 174.4370267
);

regions
(
    boxToCell
    {
        box ( 0 0 -0.1 ) ( 0.015 0.05 0.1 );
        fieldValues
        (
            volScalarFieldValue moment.0.populationBalance 0.567128698550116
            volScalarFieldValue moment.1.populationBalance 0.659798044636756
            volScalarFieldValue moment.2.populationBalance 0.796168501018439
            volScalarFieldValue moment.3.populationBalance 1.0
            volScalarFieldValue moment.4.populationBalance 1.3103698092
        );
    }
);

