FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      MG;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.000976104;
//internalField   uniform 9.76104e-05; // Hanhoun Stoichiometric case C = 0.004
//internalField   uniform 9.76104e-4; // Hanhoun Stoichiometric case C = 0.004
//internalField   uniform 0.0244026; // Hanhoun Stoichiometric case C = 0.004

boundaryField
{
    movingWall
    {
        type            zeroGradient;
    }

    fixedWalls
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}

