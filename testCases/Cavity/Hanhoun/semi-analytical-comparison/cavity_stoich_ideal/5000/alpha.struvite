FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "5000";
    object      alpha.struvite;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.0067117;

boundaryField
{
    fixedWalls
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
    movingWall
    {
        type            zeroGradient;
    }
}


