FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      PO4;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

//internalField   uniform 0.000476764; // Galbraith Stoichiometric case
//internalField   uniform 3.81411e-4;  // Hanhoun Stoichiometric case C = 0.004
internalField   uniform 0.00381411;  // Hanhoun Stoichiometric case C = 0.004

boundaryField
{
    movingWall
    {
        type            zeroGradient;
    }

    fixedWalls
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}

