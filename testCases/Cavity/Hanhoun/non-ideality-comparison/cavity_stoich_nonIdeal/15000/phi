FoamFile
{
    version     2.0;
    format      ascii;
    class       surfaceScalarField;
    location    "15000";
    object      phi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -1 0 0 0 0];

internalField   nonuniform 0();

boundaryField
{
    fixedWalls
    {
        type            calculated;
        value           uniform 0;
    }
    frontAndBack
    {
        type            empty;
        value           nonuniform 0();
    }
    movingWall
    {
        type            calculated;
        value           uniform 0;
    }
}


