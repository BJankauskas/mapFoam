FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      NH4;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

//internalField   uniform 9.05545e-05; // Galbraith Stoichiometric case C = 0.005
//internalField   uniform 7.24436e-05; // Hanhoun Stoichiometric case C = 0.004
internalField   uniform 7.24436e-4; // Hanhoun Stoichiometric case C = 0.04
//internalField   uniform 7.24436e-2; // Hanhoun Stoichiometric case C = 4

boundaryField
{
    movingWall
    {
        type            zeroGradient;
    }

    fixedWalls
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}

