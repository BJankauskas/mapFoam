FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

//internalField   uniform 0.999311; // Galbraith Stoichiometric Case C = 0.005
internalField   uniform 0.99448535; // Hanhound Stoichiometric Case C = 0.04
//internalField   uniform 0.448535; // Hanhound Stoichiometric Case C = 4

boundaryField
{
    movingWall
    {
        type            zeroGradient;
    }

    fixedWalls
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}

